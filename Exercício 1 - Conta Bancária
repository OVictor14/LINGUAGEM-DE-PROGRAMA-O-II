using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp1
{
    class Cliente
    {
        public string nome;
        public string cpf;
        public string rg;
        public string endereco;

        public string nome2;
        public string cpf2;
        public string rg2;
        public string endereco2;
    }
}

